<div class="panel_content">
	<div class="group_fields">
		<label for="page_title[]" class="autowidth validate-page_title_required help" title="{{writeString=wns.page.page_title_hint}}">{{writeString=wns.page.page_title}}</label>
		<input type="text" name="page_title[]" class="ui-widget-content input_long" />
		<input type="hidden" name="edited_languages[]" />
	</div>
	<div class="group_fields">
		<label for="link_text[]" class="help" title="{{writeString=wns.page.link_text.title}}">{{writeString=wns.page.link_text}}</label>
		<input type="text" name="link_text[]" class="ui-widget-content autowidth" />
	</div>
	<div class="group_fields">
		<label class="for_checkbox" for="is_active[]">{{writeString=wns.page.active}}</label><br />
		<input type="checkbox" name="is_active[]" />
	</div>
	<br clear="both" />
	<div>
		<label for="meta_description[]" class="autowidth">{{writeString=wns.page.meta_description}}</label>
		<input type="text" name="meta_description[]" class="ui-widget-content" style="width:48em;" />
		<br clear="both" />
		<label for="meta_keywords[]" class="autowidth">{{writeString=wns.page.meta_keywords}}</label>
		<input type="text" name="meta_keywords[]" class="ui-widget-content" style="width:48em;"/>
	</div>
	<br clear="both" />
</div>
