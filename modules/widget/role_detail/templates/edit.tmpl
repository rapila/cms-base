			<label>{{writeString=wns.created_info}}:</label><span class="created_info status"></span><br />
			<label>{{writeString=wns.updated.info}}:</label><span class="updated_info status"></span>
		</div>
	</div>
	<label for="role_key" class="validate-role_key_required validate-role_key_exists">{{writeString=wns.role.role_key}}</label>
