	<label for="description">{{writeString=wns.role.description}}</label>
	<textarea rows="2" cols="50" name="description" class="ui-widget-content"></textarea><br />
	<div id="rights">
  </div>
	<label class="toggler open">{{writeString=wns.role.legend_page_rights}}</label>
	<div class="rights_container">
